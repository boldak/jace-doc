{% extends "layout.njk" %}

{% block content %}
  
  <article class="post">
    <h1>{{ frontMatter.title }}</h1>

    
    {{ content | safe }}
  </article>
  
  <ul>
    {% for page in frontMatter.refs.items %}
      <li>
        <a href="{{ page.url }}">
          {{ page.title }}
        </a>
      </li>
    {% endfor %}
  </ul>

{% endblock %}
